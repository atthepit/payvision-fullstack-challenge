{
  "name": "payvision-challenge",
  "version": "0.0.1",
  "description": "Fullstack Software Engineer Challenge - Rendering transactions",
  "scripts": {
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "tslint --project .",
    "start": "parcel src/index.html"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": false
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.7",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^6.1.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "keywords": [
    "payvision",
    "fullstack",
    "transactions"
  ],
  "author": "Pedro Paredes <email@pparedes.com>",
  "license": "MIT"
}
